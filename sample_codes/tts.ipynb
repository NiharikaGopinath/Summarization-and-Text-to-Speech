    "from gtts import gTTS \n",
    "import os\n",
    "file = open(r\"D:\\Summarization-and-Text-to-Speech\\draft.txt\", \"r\").read().replace(\"\\n\", \" \")\n",
    "language = 'en'\n",
    "speech = gTTS(text = str(file), lang = language, slow = False)\n",
    "speech.save(\"audio.mp3\")\n",
    "os.system(\"start audio.mp3\")"
